/**app.wxss**/
page{font-size: 27rpx;font-family: 'Microsoft YaHei', '微软雅黑';line-height: 1.5;color: #666;background: #ececec}
view , scroll-view,.borderBox{box-sizing:border-box;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;-o-box-sizing:border-box;-ms-box-sizing:border-box;}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
.content{width: 100%}
input,textarea{font-size: 27rpx;font-family: 'Microsoft YaHei', '微软雅黑';background: #fff;color: #333}
.placeholder{color: #a0a0a0}
.btn_bottom{position: fixed;width: 100%;bottom:0;left:0;background:#52d2af;border-radius: 0;color: #fff;border:none;z-index: 10;height:48px}
.color-red{color: #ff7575}
.tc{text-align: center}
.tr{text-align: right}
.fb{font-weight: bold}
.h3{color: #333;font-weight: bold;font-size: 30rpx;}
/*.time{color: #a0a0a0}*/
.select{background:url('https://develop.mmmee.cn/LingMeng/resources/timeimg/right_arrow.png') center right no-repeat;background-size: 8px 16px;padding-right: 14px}
.defaultTxt{color:#bfbfbf;font-size:36rpx;text-align:center;margin-top:20px}
.defaultTxt image{width: 220rpx;height:214rpx }
@keyframes fadeIn {
    0% { opacity: 0; }
    100% { opacity: 1; }
}
.fade_in {
    animation: fadeIn 1s both;
}

.loading{text-align: center;margin:20px 0}
.loading image{width: 100px;height: 20px; }

.defaultCon{margin: 100px 120rpx 0}

.comBottom{margin: 20px 0;text-align: center;position: relative}
.comBottom:after{content:"";display: block;border-bottom:1px dotted #d1d1d1;z-index: -1;position: absolute;width: 100%;height: 0;top: 50%;left: 0}
.comBottom image{width:100px;height: 32px;background:#ececec;padding: 0 36rpx }
/*iconfont*/
@font-face {
	font-family: 'iconfont';
	src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
	font-weight: 500;
	font-style: normal;
}
.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-camera:before { content: "\e602"; }

.icon-cuowu:before { content: "\e614"; }

.icon-add:before { content: "\e60e"; }

.icon-time:before { content: "\e6e8"; }

